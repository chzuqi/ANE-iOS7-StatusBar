<extension xmlns="http://ns.adobe.com/air/extension/4.0">
  <id>cc.czq.anes.iosstatusbar</id>
  <versionNumber>1</versionNumber>
  <platforms>
    <platform name="iPhone-ARM">
        <applicationDeployment>
            <nativeLibrary>libiOSStatusBar.a</nativeLibrary>
            <initializer>iOSStatusBarExtInitializer</initializer>
            <finalizer>iOSStatusBarExtFinalizer</finalizer>
        </applicationDeployment>
    </platform>
    <platform name="iPhone-x86">
        <applicationDeployment>
            <nativeLibrary>libiOSStatusBar.a</nativeLibrary>
            <initializer>iOSStatusBarExtInitializer</initializer>
            <finalizer>iOSStatusBarExtFinalizer</finalizer>
        </applicationDeployment>
    </platform>

